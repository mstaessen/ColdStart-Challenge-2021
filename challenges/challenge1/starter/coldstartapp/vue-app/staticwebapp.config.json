{
  "routes": [
    {
      "route": "/api/catalog"
    },
    {
      "route": "/login",
      "serve": "/.auth/login/github",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/logout",
      "serve": "/.auth/logout",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/order/*",
      "serve": "/index.html",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  }
}