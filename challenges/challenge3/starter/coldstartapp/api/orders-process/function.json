{
  "bindings": [
    {
      "type": "queueTrigger",
      "direction": "in",
      "name": "orderQueueItem",
      "queueName": "customer-orders",
      "connection": "STORAGE_CONNECTIONSTRING"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "orderDocument",
      "connectionStringSetting": "COSMOSDB_CONNECTIONSTRING",
      "databaseName": "ColdStartChallenge",
      "collectionName": "Orders",
      "createIfNotExists": true
    }
  ]
}